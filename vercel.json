{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/dashboard/index.html"
    },
    {
      "src": "/api/upload",
      "dest": "/api/upload.py"
    },
    {
      "src": "/api/view",
      "dest": "/api/view.py"
    }
  ]
}
